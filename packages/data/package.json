{
  "name": "@graphql-inspector/data",
  "version": "0.0.0-PLACEHOLDER",
  "description": "Tooling for GraphQL. Compare GraphQL Schemas, check documents, find breaking changes, find similar types.",
  "keywords": [
    "graphql",
    "graphql-inspector",
    "tools"
  ],
  "sideEffects": false,
  "main": "dist/index.js",
  "author": {
    "name": "Kamil Kisiela",
    "email": "kamil.kisiela@gmail.com",
    "url": "https://github.com/kamilkisiela"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "kamilkisiela/graphql-inspector"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "types": "graphql-codegen --config codegen.yml",
    "clean": "rm -rf dist/",
    "prebuild": "yarn clean && yarn types",
    "prepublishOnly": "yarn build"
  },
  "peerDependencies": {
    "graphql": "^0.13.0 || ^14.0.0"
  },
  "dependencies": {
    "apollo-graphql": "0.3.3",
    "async-retry": "1.2.3",
    "axios": "0.19.0",
    "graphql-extensions": "0.9.1",
    "graphql-scalars": "1.0.1",
    "graphql-tools": "4.0.5",
    "knex": "0.19.2",
    "pg": "7.12.1"
  },
  "devDependencies": {
    "@graphql-codegen/add": "1.6.1",
    "@graphql-codegen/cli": "1.6.1",
    "@graphql-codegen/typescript": "1.6.1",
    "@graphql-codegen/typescript-resolvers": "1.6.1",
    "@types/async-retry": "1.4.1",
    "@types/graphql": "14.2.3",
    "@types/jest": "24.0.17",
    "@types/node": "12.7.2",
    "jest": "24.8.0",
    "ts-jest": "24.0.2",
    "typescript": "3.5.3"
  }
}
